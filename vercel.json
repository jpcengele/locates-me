{
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/.next",
    "framework": "nextjs",
    "functions": {
        "api/*.py": {
            "runtime": "python3.9"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ]
}